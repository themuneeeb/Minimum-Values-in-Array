#include <iostream>
using namespace std;
int main()
{
	int A[100], size, max, i;
	cout << " Enter the size of Array :" << endl;
	cin >> size;
	cout << " Enter the values in Array : " << endl;
	
	for ( i = 0; i < size; i++)
	{
		cin >> A[i];
	}
	max = A[0];
	for (int i = 0; i < size; ++i)
	{
		if (max < A[i])
			max = A[i];
		
	}
	cout << " The maximum value in Array is : " << max << endl;
	system("pause");
}
